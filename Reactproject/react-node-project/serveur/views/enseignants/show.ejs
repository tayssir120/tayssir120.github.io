<%- include('../partials/header.ejs') %>
<div class="hero hero3">
</div>
<div class="page-single movie-single movie_single">
	<div class="container">
		<div class="row ipad-width2">
			<div class="col-md-4 col-sm-12 col-xs-12">
				<div class="movie-img sticky-sb">
					<img src="<%= enseignant.coverImagePath %>" alt="">
					<div class="movie-btn">	
						<div>
							<div><a href="/enseignants/<%= enseignant.id %>/edit" class="item item-1 yellowbtn"> <i class="ion-edit"></i> Edit</a></div>
						</div>
						<br>
							<div><%- include('../partials/deleteForm',
								{ url: `/enseignants/${enseignant.id}` }) %></div>
					</div>
				</div>
			</div>
			<div class="col-md-8 col-sm-12 col-xs-12">
				<div class="movie-single-ct main-content">
					<h1 class="bd-hd"><%= enseignant.name %></h1>
				<br>
				<br>
					<div class="movie-rate">
						<h3 style="color: #F0F8FF ;">Enseignant</h3>
						<h2 style="color: #F0F8FF ;"><%= enseignant.name %></h2>
		  </div>
				<div class="mvcast-item">											
					<div class="cast-it">
						<div class="cast-left">
							<img src="images/uploads/cast1.jpg" alt="">
							<p style="color: rgb(74, 104, 202);">Enseignant:</p>
						</div>
						<p><%= enseignant.name %></p>
					</div>
					<div class="cast-it">
						<div class="cast-left">
							<img src="images/uploads/cast1.jpg" alt="">
							<p style="color: rgb(74, 104, 202);">Date of Birth:</p>
						</div>
						<p><%= enseignant.DateofBirth %></p>
					</div>
					<div class="cast-it">
						<div class="cast-left">
							<img src="images/uploads/cast1.jpg" alt="">
							<p style="color: rgb(74, 104, 202);">Place of Birth:</p>
						</div>
						<p><%= enseignant.placeofBirth %></p>
					</div>
					<div class="cast-it">
						<div class="cast-left">
							<img src="images/uploads/cast1.jpg" alt="">
							<p style="color: rgb(74, 104, 202);">Description:</p>
						</div>
						<p><%= enseignant.description %></p>
					</div>
					
						</div>
						
					
		</div>
	</div>
</div>
</div>
