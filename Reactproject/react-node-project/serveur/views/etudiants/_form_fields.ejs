
<div class="form-row">
  <div class="form-item">
    <label>Name</label>
    <input type="text" name="name" value="<%= etudiant.name %>">
  </div>
</div>
<div class="form-row">
  <div class="form-item">
    <label>Date of Birth</label>
    <input type="text" name="DateofBirth" value="<%= etudiant.DateofBirth %>">
  </div>
</div>
<div class="form-row">
  <div class="form-item">
    <label>Place of Birth</label>
    <input type="text" name="placeofBirth" value="<%= etudiant.placeofBirth %>">
  </div>
</div>
<div class="form-row">
  <div class="form-item form-item-no-grow">
    <label>Poster</label>
    <input type="file" name="cover" class="book-cover filepond">
  </div>
</div>
<div class="form-row">
  <div class="form-item">
    <label>Description</label>
    <input type="text" name="description" value="<%= etudiant.description %>">
  </div>
</div>